{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Error",
        "System": "Error"
      }
    },
    "WriteTo": [ { "Name": "Console" } ]
  },
  "FeatureSettings": {
    "IsFeatureXEnabled": false,
    "WelcomeMessage": "Hello, world!"
  }
}
